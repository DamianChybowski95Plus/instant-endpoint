{
    "name": "instant-endpoint",
    "version": "0.1.16",
    "scripts": {
        "devEnv-dev": "next dev",
        "devEnv-build": "next build",
        "devEnv-start": "next start",
        "devEnv-lint": "next lint",
        "dev": "npm run publication-build -- --watch",
        "publication-build": "tsup src/index.ts --format cjs,esm --dts",
        "publish-package": "npm run publication-build && npx beachball change && npx beachball bump && npx beachball publish"
    },
    "dependencies": {
        "next": "14.2.3",
        "utility-types": "^3.11.0",
        "zod": "^3.23.5"
    },
    "devDependencies": {
        "tsup": "^8.0.2",
        "react": "^18",
        "react-dom": "^18",
        "beachball": "2.43.1",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-next": "14.2.3",
        "typescript": "^5"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "next"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DamianChybowski95Plus/instant-endpoint"
    },
    "packageManager": "npm@10.2.0",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
}
