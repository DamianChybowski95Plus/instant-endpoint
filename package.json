{
    "name": "instant-endpoint",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "publish-build": "tsup src/index.ts --format cjs,esm --dts",
        "publish-dev": "npm run publish-build -- --watch",
        "publish-packages": "turbo run publish-build && changeset version && changeset publish"
    },
    "dependencies": {
        "@changesets/cli": "^2.27.1",
        "next": "14.2.3",
        "react": "^18",
        "react-dom": "^18",
        "tsup": "^8.0.2",
        "utility-types": "^3.11.0",
        "zod": "^3.23.5"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-next": "14.2.3",
        "typescript": "^5"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "npm@10.2.0",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
}
